{% extends 'base.html.twig' %}

{% block title %}Maincustomer index
{% endblock %}

{% block body %}
	<h1>Maincustomer index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Contact</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for maincustomer in maincustomers %}
				<tr>
					<td>{{ maincustomer.id }}</td>
					<td>{{ user.maincustomer.contact }}</td>
					<td>
						<a href="{{ path('app_maincustomer_show', {'id': maincustomer.id}) }}">show</a>
						<a href="{{ path('app_maincustomer_edit', {'id': maincustomer.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_maincustomer_new') }}">Create new</a>
{% endblock %}
